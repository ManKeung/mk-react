{
    "name": "mk-react",
    "private": true,
    "version": "1.0.0",
    "description": "vite + react + ts项目基础配置",
    "warName": "vite",
    "homepage": "https://mankeung.github.io/docs/mk-react",
    "repository": {
        "type": "git",
        "url": "https://github.com/mankeung/mk-react"
    },
    "author": "mankeung",
    "keywords": [
        "react",
        "vite",
        "typescript",
        "ts",
        "redux",
        "saga",
        "axios"
    ],
    "scripts": {
        "dev": "cross-env NODE_ENV=dev vite --mode dev",
        "build:test": "tsc && cross-env NODE_ENV=test vite build --mode prod",
        "build": "tsc && cross-env NODE_ENV=prod vite build --mode prod",
        "preview": "vite preview",
        "lint:eslint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
        "lint:prettier": "prettier --write  \"src/**/*.{js,jsx,ts,tsx,json,css,less,scss,vue,html,md}\"",
        "lint:style": "stylelint --fix \"src/**/*.{scss,less,vue,css}\" --cache --cache-location node_modules/.cache/stylelint/",
        "lint": "pnpm lint:prettier && pnpm lint:style && pnpm lint:eslint",
        "prepare": "node ./scripts/prepare.js",
        "precommit": "node ./scripts/precommit.js",
        "war": "node ./scripts/war.js",
        "new": "plop"
    },
    "dependencies": {
        "amfe-flexible": "^2.2.1",
        "axios": "^1.0.0",
        "crypto-js": "^4.1.1",
        "hotkeys-js": "^3.10.0",
        "immutable": "^4.1.0",
        "jsencrypt": "^3.2.1",
        "mitt": "^3.0.0",
        "normalize.css": "^8.0.1",
        "nprogress": "^0.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.4",
        "react-router-dom": "^6.4.2",
        "redux": "^4.2.0",
        "redux-immutable": "^4.0.0",
        "redux-saga": "^1.2.1",
        "xss": "^1.0.14"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "@types/crypto-js": "^4.1.1",
        "@types/node": "^18.8.2",
        "@types/nprogress": "^0.2.0",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@types/redux-immutable": "^4.0.3",
        "@typescript-eslint/eslint-plugin": "^5.39.0",
        "@typescript-eslint/parser": "^5.39.0",
        "@vitejs/plugin-legacy": "^2.2.0",
        "@vitejs/plugin-react": "^2.1.0",
        "chalk": "4",
        "cross-env": "^7.0.3",
        "enquirer": "^2.3.6",
        "eslint": "^8.24.0",
        "eslint-plugin-react": "^7.31.8",
        "eslint-plugin-react-hooks": "^4.6.0",
        "folder-zip-sync": "^1.0.5",
        "husky": "^8.0.1",
        "i18next": "^21.10.0",
        "mockjs": "^1.1.0",
        "plop": "^3.1.1",
        "postcss-html": "^1.5.0",
        "postcss-pxtorem": "^6.0.0",
        "postcss-scss": "^4.0.5",
        "prettier": "^2.7.1",
        "react-i18next": "^11.18.6",
        "rollup-plugin-visualizer": "^5.8.2",
        "sass": "^1.55.0",
        "stylelint": "^14.13.0",
        "stylelint-config-standard": "^28.0.0",
        "stylelint-order": "^5.0.0",
        "stylelint-scss": "^4.3.0",
        "typescript": "^4.6.4",
        "unplugin-auto-import": "^0.11.2",
        "vite": "^3.1.0",
        "vite-plugin-banner": "^0.5.0",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-html": "^3.2.0",
        "vite-plugin-mock": "^2.9.6",
